{
  "authors": [
    {
      "name": "nZEDb Team"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app-cake/src/",
      "nzedb\\": "nzedb/",
      "zed\\": "app-zed"
    },
    "classmap": [
      "www/pages/",
      "libs/PEAR/Net_NNTP/NNTP/",
      "libs/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "app-cake/tests/",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
    }
  },
  "config": {
    "sort-packages": false,
    "optimize-autoloader": true,
    "fxp-asset": {
      "installer-paths": {
        "npm-asset-library": "www/themes/shared/libs",
        "bower-asset-library": "www/themes/shared/libs"
      }
    },
    "process-timeout": 3600,
    "preferred-install": {
      "*": "source"
    },
    "vendor-dir": "vendor"
  },
  "description": "A Usenet Indexer",
  "extra": {
    "branch-alias": {
      "dev-dev": "0.x-dev"
    }
  },
  "homepage": "http://nzedb.com",
  "keywords": [
    "nZEDb",
    "usenet",
    "indexer"
  ],
  "license": "GPL-3.0",
  "minimum-stability": "dev",
  "name": "nzedb/nzedb",
  "non-feature-branches": [
    "Latest-*"
  ],
  "repositories": [
     {
      "type": "vcs",
      "url": "https://github.com/nZEDb/Git.php"
    },
    {
      "type": "vcs",
      "url": "https://github.com/zeebinz/rarinfo"
    }
  ],
  "require": {
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-filter": "*",
    "ext-gd": "*",
    "ext-hash": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mysqlnd": "*",
    "ext-openssl": "*",
    "ext-pcre": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-session": "*",
    "ext-simplexml": "*",
    "ext-sockets": "*",
    "ext-spl": "*",
    "ext-xmlwriter": "*",
    "ext-zlib": "*",
    "b3rs3rk/steamfront": "dev-master",
    "barracudanetworks/forkdaemon-php": "^1.0.7",
    "cakephp/authentication": "dev-master",
    "cakephp/authorization": "dev-master",
    "cakephp/cakephp": "3.8.*",
    "cakephp/migrations": "^2.0.0",
    "cakephp/plugin-installer": "^1.0",
    "composer-plugin-api": "^1.0.0",
    "dborsatto/php-giantbomb": "dev-master",
    "guzzlehttp/guzzle": "^6.3",
    "ivanamat/cakephp3-captcha": "dev-master",
    "james-heinrich/getid3": "^1.9.12",
    "joshpinkney/tv-maze-php-api": "dev-master",
    "maiconpinto/cakephp-adminlte-theme": "dev-master",
    "mhor/php-mediainfo": "^5.0@dev",
    "mobiledetect/mobiledetectlib": "2.*",
    "moinax/tvdb": "^1.0.4",
    "nzedb/git.php": "dev-master",
    "php": "^7.1.3",
    "php-tmdb/api": "^2.1",
    "phpmailer/phpmailer": "^6.0.2",
    "ramsey/uuid": "^4.0@dev",
    "zeebinz/rarinfo": "~2.5.0"
  },
  "require-dev": {
    "cakephp/bake": "^1.9.0",
    "cakephp/cakephp-codesniffer": "^3.0",
    "cakephp/debug_kit": "^3.17.0",
    "josegonzalez/dotenv": "3.*",
    "psy/psysh": "@stable"
  },
  "scripts": {
    "post-install-cmd": [
      "nzedb\\build\\Composer::postInstallCmd"
    ],
    "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump",
    "check": [
      "@test",
      "@cs-check"
    ],
    "cs-check": "phpcs --colors -p --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
    "cs-fix": "phpcbf --colors --standard=vendor/cakephp/cakephp-codesniffer/CakePHP src/ tests/",
    "test": "phpunit --colors=always"
  },
  "suggest": {
    "ext-php-yenc": "Improves yEnc decoding speed for PHP 7.0+ (and 5.6)",
    "ext-xdebug": "For developement work",
    "markstory/asset_compress": "An asset compression plugin which provides file concatenation and a flexible filter system for preprocessing and minification.",
    "dereuromark/cakephp-ide-helper": "After baking your code, this keeps your annotations in sync with the code evolving from there on for maximum IDE and PHPStan compatibility."
  },
  "type": "project"
}
